{
  "productName": "angular-electron",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "win": {
    "icon": "dist",
    "target": [
      "portable"
    ],
    "publish": ["github"]
  },
  "publish": {
    "provider": "github",
    "private": false,
    "releaseType": "release"
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ],
    "publish": ["github"]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "deb"
    ],
    "publish": ["github"]
  }
}
